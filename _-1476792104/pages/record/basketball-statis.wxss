.bg-183 {
    z-index: 9;
}

.statusBar {
    position: relative;
    z-index: 99;
}

.icon-group,.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 64rpx;
}

.icon-group {
    position: fixed;
    left: 0;
    width: 174rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid hsla(0,0%,98%,.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: hsla(0,0%,98%,.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #f2f9ff;
}

.match-banner {
    width: 100%;
    height: 520rpx;
    background: #fff;
    border-radius: 20rpx;
    overflow: hidden;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,.08);
}

.match-banner image {
    width: 100%;
    height: 100%;
}

.match-title {
    width: 100%;
    height: 120rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,.08);
    margin: 25rpx 0 20rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    justify-content: space-between;
}

.match-title,.match-title .title-right {
    display: flex;
    align-items: center;
}

.match-title .title-right image {
    width: 20rpx;
    height: 25rpx;
    margin-right: 10rpx;
}

.my-match {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.my-match .title {
    margin: 10rpx 0;
}

.my-match .match-item-1 {
    width: 690rpx;
    height: 283rpx;
    border-radius: 20rpx;
    background: linear-gradient(90deg,#3ca6fd,#2f5fff);
    box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(53,131,254,.22);
    display: flex;
    align-items: center;
    justify-content: center;
}

.my-match .match-item-1 .item-content {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.my-match .match-item-1 .item-content image {
    width: 90rpx;
    height: 90rpx;
}

.my-match .match-item-1 .line {
    width: 4rpx;
    height: 120rpx;
    left: 375rpx;
    margin: 0 83rpx;
    border-radius: 20rpx;
    background-color: hsla(0,0%,100%,.7);
}

.my-match .match-item-2 {
    width: 690rpx;
    height: 540rpx;
    border-radius: 20rpx;
    background: #fff;
    margin-top: 30rpx;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,.08);
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    padding: 0 15rpx;
    box-sizing: border-box;
}

.my-match .match-item-2 .item-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 45rpx;
    height: 215rpx;
    width: 130rpx;
    white-space: nowrap;
}

.my-match .match-item-2 .item-content image {
    width: 120rpx;
    height: 120rpx;
}

.my-match .btn-group {
    display: flex;
    justify-content: space-between;
    margin: 70rpx 0;
}

.my-match .btn-group .btn {
    width: 334rpx;
    height: 110rpx;
    background: linear-gradient(90deg,#3ca6fd,#2f5fff);
    border-radius: 55rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(53,131,254,.22);
}

.my-match .btn-group .btn:last-child {
    background: linear-gradient(90deg,#5be89b,#2eccbd);
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(18,179,159,.2);
}