.bg-183 {
    z-index: 9;
}

.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
    z-index: 99;
}

.icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #F2F9FF;
}

.match-banner {
    width: 100%;
    height: 520rpx;
    background: #fff;
    border-radius: 20rpx;
    overflow: hidden;
    box-shadow: 0 16rpx 16rpx 0 rgba(85,145,252,0.08);
}

.match-banner image {
    width: 100%;
    height: 100%;
}

.match-title {
    width: 100%;
    height: 120rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0 16rpx 16rpx 0 rgba(85,145,252,0.08);
    margin: 25rpx 0 20rpx 0;
    padding: 0 30rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.match-title .title-right {
    display: flex;
    align-items: center;
}

.match-title .title-right image {
    width: 20rpx;
    height: 25rpx;
    margin-right: 10rpx;
}

.my-match {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.my-match .title {
    margin: 10rpx 0;
}

.my-match .match-item-1 {
    width: 690rpx;
    height: 283rpx;
    border-radius: 20rpx;
    background: linear-gradient(to right,#3CA6FD,#2F5FFF);
    box-shadow: 0 8rpx 20rpx 0 rgba(53,131,254,0.22);
    display: flex;
    align-items: center;
    justify-content: center;
}

.my-match .match-item-1 .item-content {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.my-match .match-item-1 .item-content image {
    width: 90rpx;
    height: 90rpx;
}

.my-match .match-item-1 .line {
    width: 4rpx;
    height: 120rpx;
    left: 375rpx;
    margin: 0 83rpx;
    border-radius: 20rpx;
    background-color: rgba(255,255,255,0.7);
}

.my-match .match-item-2 {
    width: 690rpx;
    height: 540rpx;
    border-radius: 20rpx;
    background: #fff;
    margin-top: 30rpx;
    box-shadow: 0 16rpx 16rpx 0 rgba(85,145,252,0.08);
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    padding: 0 15rpx;
    box-sizing: border-box;
}

.my-match .match-item-2 .item-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 45rpx;
    height: 215rpx;
    width: 130rpx;
    white-space: nowrap;
}

.my-match .match-item-2 .item-content image {
    width: 120rpx;
    height: 120rpx;
}

.my-match .btn-group {
    display: flex;
    justify-content: space-between;
    margin: 70rpx 0;
}

.my-match .btn-group .btn {
    width: 334rpx;
    height: 110rpx;
    background: linear-gradient(to right,#3CA6FD,#2F5FFF);
    border-radius: 55rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

.my-match .btn-group .btn:last-child {
    background: linear-gradient(to right,#5BE89B,#2ECCBD);
    box-shadow: 0 8rpx 16rpx 0 rgba(18,179,159,0.2);
}