.bg-183 {
    z-index: 9;
}

.statusBar {
    position: relative;
    z-index: 99;
}

.icon-group,.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 64rpx;
}

.icon-group {
    position: fixed;
    left: 0;
    width: 174rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid hsla(0,0%,98%,.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: hsla(0,0%,98%,.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #f2f9ff;
}

.match-banner {
    width: 100%;
    height: 534rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,.08);
}

.match-banner image {
    width: 100%;
    height: 100%;
}

.match-title {
    width: 100%;
    height: 120rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,.08);
    margin: 25rpx 0 20rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    justify-content: space-between;
}

.match-title,.match-title .title-right {
    display: flex;
    align-items: center;
}

.match-title .title-right image {
    width: 20rpx;
    height: 25rpx;
    margin-right: 10rpx;
}

.my-match {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.my-match .title {
    margin: 10rpx 0;
}

.my-match .match-item-1 {
    margin-top: 16rpx;
}

.my-match .match-item-1 .item-top {
    display: flex;
    justify-content: space-between;
}

.my-match .match-item-1 .list-item {
    width: 100%;
    height: 342rpx;
    background: linear-gradient(45deg,#3ca6fd,#2f5fff);
    border-radius: 20rpx;
    box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(17,85,233,.22);
    margin-top: 10rpx;
    position: relative;
}

.my-match .match-item-1 .list-item .item-top {
    display: flex;
    justify-content: space-between;
}

.my-match .match-item-1 .list-item .item-top .item-top-left {
    position: relative;
    width: 178rpx;
    height: 58rpx;
    padding-left: 20rpx;
}

.my-match .match-item-1 .list-item .item-top .item-top-right {
    display: flex;
    align-items: center;
    position: absolute;
    right: 30rpx;
    top: 16rpx;
}

.my-match .match-item-1 .list-item .item-top .item-top-right image {
    width: 20rpx;
    height: 26rpx;
    margin-right: 8rpx;
}

.my-match .match-item-1 .list-item .item-middle {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 10rpx;
}

.my-match .match-item-1 .list-item .item-middle .item-middle-info {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.my-match .match-item-1 .list-item .item-middle .item-middle-info .Merriweather-Book {
    text-shadow: 1rpx 2rpx 5rpx rgba(0,0,0,.2);
}

.my-match .match-item-1 .list-item .item-middle .item-middle-info view {
    display: flex;
    align-items: center;
    margin-top: 10rpx;
}

.my-match .match-item-1 .list-item .item-middle .item-middle-info view image {
    width: 38rpx;
    height: 38rpx;
}

.my-match .match-item-1 .list-item .item-middle .line {
    width: 2rpx;
    height: 30rpx;
    border-radius: 2rpx;
    background-color: hsla(0,0%,100%,.7);
    margin: 0 25rpx;
}

.my-match .match-item-1 .list-item .item-bottom {
    width: 100%;
    height: 110rpx;
    border-radius: 20rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 39rpx 0 30rpx;
    box-sizing: border-box;
    position: absolute;
    bottom: 0;
}

.my-match .match-item-1 .list-item .item-bottom .score {
    color: #316cfe;
    margin-top: -20rpx;
    text-shadow: 1rpx 3rpx 10rpx rgba(0,0,0,.2);
}

.my-match .match-item-2 {
    margin-top: 26rpx;
}

.my-match .match-item-2 .best-record {
    width: 100%;
    height: 210rpx;
    border-radius: 20rpx;
    background: linear-gradient(45deg,#da9afb,#987efb);
    box-shadow: 0rpx 5rpx 12rpx 0rpx rgba(179,137,251,.36),inset 0rpx -1rpx 3rpx 0rpx rgba(214,152,251,.8);
    display: flex;
    align-items: center;
    margin-top: 10rpx;
}

.my-match .match-item-2 .best-record .record-item-1 {
    display: flex;
    flex-direction: column;
    margin: 0 65rpx;
}

.my-match .match-item-2 .best-record .record-item-1 image {
    width: 100rpx;
    height: 100rpx;
    border: 4rpx solid #fff;
    border-radius: 50%;
    box-shadow: 0rpx 8rpx 10rpx 0rpx #b389fb;
    margin-bottom: 10rpx;
}

.my-match .match-item-2 .best-record .record-item-2 {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: -30rpx;
}

.my-match .match-item-2 .best-record .record-item-2 .font-style {
    text-shadow: 1rpx 3rpx 10rpx rgba(0,0,0,.3);
}

.my-match .match-item-2 .best-record .line {
    width: 4rpx;
    height: 30rpx;
    border-radius: 2rpx;
    background-color: hsla(0,0%,100%,.7);
    margin: 0 50rpx;
}

.my-match .nationwide-rank {
    display: flex;
    flex-direction: column;
    padding: 50rpx 0 32rpx;
}

.my-match .nationwide-rank .list .rank:nth-child(1) .rank-left .Merriweather {
    border-color: #d9c38c;
    color: #d9c38c;
}

.my-match .nationwide-rank .list .rank:nth-child(2) .rank-left .Merriweather {
    border-color: #999;
    color: #999;
}

.my-match .nationwide-rank .list .rank:nth-child(3) .rank-left .Merriweather {
    border-color: #d5a187;
    color: #d5a187;
}

.my-match .nationwide-rank .rank {
    width: 690rpx;
    height: 220rpx;
    border-radius: 20rpx;
    background: #fff;
    margin: 20rpx 0;
    display: flex;
    align-items: center;
    left: 30rpx;
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(17,85,233,.1);
}

.my-match .nationwide-rank .rank:nth-child(1) .rank-left .Merriweather {
    border: 4rpx solid #d9c38c;
    color: #dbbb81;
}

.my-match .nationwide-rank .rank:nth-child(2) .rank-left .Merriweather {
    border: 4rpx solid #bfbfbf;
    color: #999;
}

.my-match .nationwide-rank .rank:nth-child(3) .rank-left .Merriweather {
    border: 4rpx solid #d5a187;
    color: #d5a187;
}

.my-match .nationwide-rank .rank .rank-left {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 30rpx;
}

.my-match .nationwide-rank .rank .rank-left .Merriweather {
    width: 98rpx;
    height: 98rpx;
    border: 4rpx solid #bbb;
    border-radius: 50%;
    text-align: center;
    line-height: 98rpx;
}

.my-match .nationwide-rank .rank .rank-right {
    display: flex;
    flex-direction: column;
    width: 500rpx;
}

.my-match .nationwide-rank .rank .rank-right .rank-right-top {
    display: flex;
    align-items: center;
}

.my-match .nationwide-rank .rank .rank-right .rank-right-top image {
    width: 80rpx;
    height: 80rpx;
    border: 4rpx solid #fff;
    border-radius: 50%;
    margin: 20rpx;
    box-shadow: 0rpx 8rpx 10rpx 0rpx rgba(53,131,254,.1);
}

.my-match .nationwide-rank .rank .rank-right .rank-right-top .user-name {
    width: 170rpx;
}

.my-match .nationwide-rank .rank .rank-right .rank-right-bottom {
    display: flex;
    border-top: 1rpx solid #d8d8d8;
    justify-content: center;
}

.my-match .nationwide-rank .rank .rank-right .rank-right-bottom .icon-item {
    flex: 1;
    display: flex;
    align-items: center;
    margin-top: 15rpx;
}

.my-match .nationwide-rank .rank .rank-right .rank-right-bottom .icon-item image {
    width: 38rpx;
    height: 38rpx;
    margin-right: 5rpx;
}

.my-match .btn-group {
    display: flex;
    justify-content: space-between;
    margin: 70rpx 0;
}

.my-match .btn-group .btn {
    width: 334rpx;
    height: 110rpx;
    background: linear-gradient(90deg,#3ca6fd,#2f5fff);
    border-radius: 55rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(53,131,254,.22);
}

.my-match .btn-group .btn:last-child {
    background: linear-gradient(90deg,#5be89b,#2eccbd);
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(18,179,159,.2);
}