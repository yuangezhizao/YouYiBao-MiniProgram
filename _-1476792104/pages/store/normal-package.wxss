.confirm-box {
    width: 540rpx;
    height: 469rpx;
    background: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0 40rpx;
    box-sizing: border-box;
}

.confirm-box .close {
    width: 42rpx;
    height: 70rpx;
    position: absolute;
    right: 10rpx;
    top: -70rpx;
}

.confirm-box .top {
    width: 100%;
    height: 136rpx;
    position: absolute;
    top: 0;
    text-align: center;
    line-height: 136rpx;
}

.confirm-box .content {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    margin: 130rpx 0 40rpx;
    text-align: center;
}

.confirm-box .btn_group_cus {
    width: 290rpx;
    height: 96rpx;
    border-radius: 55rpx;
    text-align: center;
    line-height: 96rpx;
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(53,131,254,.22);
}

.confirm-box .button_group {
    display: flex;
    justify-content: center;
    width: 100%;
}

.confirm-box .button_group .button {
    width: 210rpx;
    height: 90rpx;
    border-radius: 55rpx;
    background: linear-gradient(45deg,#9a9a9a,#575757);
    text-align: center;
    line-height: 90rpx;
    box-shadow: 0rpx 8rpx 16rpx 0rpx hsla(0,0%,63.1%,.22);
    margin: 0 15rpx;
}

.confirm-box .button_group .button:last-child {
    background: linear-gradient(45deg,#3ca6fd,#2f5fff);
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(53,131,254,.22);
}

.bg-183 {
    z-index: 9;
}

.statusBar {
    position: relative;
    z-index: 99;
}

.icon-group,.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 64rpx;
}

.icon-group {
    position: fixed;
    left: 0;
    width: 174rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid hsla(0,0%,98%,.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: hsla(0,0%,98%,.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #f2f9ff;
    padding-bottom: 50rpx;
}

.package-top {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 100rpx;
    background: #fff;
}

.package-top image {
    width: 506rpx;
    height: 260rpx;
}

.package-top .package-top-bottom {
    width: 506rpx;
    display: flex;
    justify-content: space-between;
    margin: 23rpx 0 47rpx;
}

.package-top .package-top-bottom .stock {
    background: #3879fe;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    border-radius: 25rpx;
    padding: 0 15rpx;
}

.package-info {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 35rpx;
    border-radius: 20rpx;
    margin-top: -10rpx;
    background: linear-gradient(0deg,hsla(0,0%,100%,.1),#e3ecff);
}

.package-info .package-detail {
    display: flex;
    flex-direction: column;
    width: 690rpx;
    height: 319rpx;
    border-radius: 10rpx;
    box-shadow: 0rpx 5rpx 10rpx 0rpx rgba(85,145,252,.2);
    padding: 46rpx 55rpx;
    box-sizing: border-box;
    margin-bottom: 20rpx;
}

.package-info .package-detail .row {
    display: flex;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.package-info .tips {
    width: 690rpx;
    line-height: 40rpx;
}

.package-info .count {
    display: flex;
    margin: 30rpx 0 47rpx;
}

.package-info .count .down {
    width: 78rpx;
    height: 78rpx;
    border-radius: 10rpx;
    background: #3376fe;
    text-align: center;
    line-height: 78rpx;
}

.package-info .count input {
    width: 160rpx;
    height: 78rpx;
    border-radius: 10rpx;
    border: 1rpx solid #3376fe;
    margin: 0 20rpx;
    text-align: center;
    color: #3376fe;
}

.package-info .count .active {
    background: #bbb;
}

.package-info .agree {
    display: flex;
    align-items: center;
    margin-bottom: 30rpx;
}

.package-info .agree .checkbox {
    border-radius: 50%;
    width: 28rpx;
    height: 28rpx;
    border: 1rpx solid #999;
    margin-right: 5rpx;
    position: relative;
    transition: all .5s;
}

.package-info .agree .checkbox.checked {
    background-color: #2660e7;
    border-color: #2660e7;
}

.package-info .agree .checkbox.checked:after {
    content: "";
    width: 15rpx;
    height: 6rpx;
    border: 1rpx solid #fff;
    border-top: 0;
    border-right: 0;
    position: absolute;
    display: block;
    left: 6rpx;
    top: 10rpx;
    transform: rotate(-45deg) scale(1);
}

.package-info .package-pay {
    width: 340rpx;
    height: 96rpx;
    border-radius: 55rpx;
    text-align: center;
    line-height: 96rpx;
    left: 205rpx;
    box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(0,67,173,.22);
}