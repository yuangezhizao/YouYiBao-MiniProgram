.bg-183 {
    z-index: 9;
}

.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
    z-index: 99;
}

.icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #F2F9FF;
}

.store-info {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 100rpx;
    background: #fff;
}

.store-info .select-store {
    display: flex;
    flex-direction: column;
    align-items: center;
    position: absolute;
    right: 20rpx;
    top: 188rpx;
}

.store-info .select-store image {
    width: 33rpx;
    height: 33rpx;
}

.store-info .store-logo {
    width: 258rpx;
    height: 158rpx;
    margin-bottom: 10rpx;
}

.store-info .store-address {
    display: flex;
    align-items: flex-start;
    margin-top: 32rpx;
    padding: 0 50rpx;
}

.store-info .store-address image {
    width: 20rpx;
    min-width: 20rpx;
    height: 25rpx;
    margin: 8rpx 10rpx 0 0;
}

.store-info .line {
    width: 42rpx;
    height: 4rpx;
    background: #bbb;
    margin: 18rpx 0;
}

.store-info .store-mobile {
    display: flex;
    align-items: center;
    margin-bottom: 49rpx;
}

.store-info .store-mobile image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 7rpx;
}

.latest-activity {
    display: flex;
    flex-direction: column;
    margin-top: -10rpx;
    border-radius: 20rpx;
    background: linear-gradient(to top,rgba(255,255,255,0.1),#e3ecff);
    height: 510rpx;
    padding: 0 30rpx;
}

.latest-activity .title {
    margin: 37rpx 0 22rpx;
    display: flex;
    justify-content: space-between;
}

.latest-activity .activity-banner {
    width: 100%;
    height: 350rpx;
    position: relative;
    border-radius: 16rpx;
    overflow: hidden;
    margin-bottom: 36rpx;
}

.latest-activity .activity-banner .activity-banner-title {
    width: 100%;
    position: absolute;
    bottom: 0;
    padding: 20rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: linear-gradient(to top,rgba(34,34,34,0.35),rgba(232,241,254,0));
}

.member-package {
    display: flex;
    flex-direction: column;
    margin-top: -10rpx;
    border-radius: 20rpx;
    background: linear-gradient(to top,rgba(255,255,255,0.1),#e3ecff);
    height: 427rpx;
}

.member-package .title {
    margin: 37rpx 0 22rpx 30rpx;
}

.member-package .package-list {
    display: flex;
    margin-left: 30rpx;
}

.member-package .package-list .list-item {
    display: flex;
    flex-direction: column;
    margin-right: 30rpx;
}

.member-package .package-list .list-item image {
    width: 330rpx;
    height: 190rpx;
    border-radius: 20rpx;
    margin-bottom: 22rpx;
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

.normal-package {
    display: flex;
    flex-direction: column;
    background: linear-gradient(to top,rgba(255,255,255,0.1),#e3ecff);
    border-radius: 20rpx;
    margin-bottom: 50rpx;
}

.normal-package .title {
    margin: 37rpx 0 0 30rpx;
}

.normal-package .package-item {
    width: 690rpx;
    height: 170rpx;
    border-radius: 10rpx;
    box-shadow: 0 6rpx 10rpx 4rpx #e9f1ff;
    display: flex;
    margin: 15rpx 30rpx;
    background: #fff;
    position: relative;
    padding: 10rpx;
    box-sizing: border-box;
}

.normal-package .package-item .img-group {
    display: flex;
}

.normal-package .package-item .img-group .type {
    width: 100rpx;
    height: 150rpx;
}

.normal-package .package-item .img-group .img {
    width: 207rpx;
    height: 150rpx;
}

.normal-package .package-item .item-info {
    display: flex;
    flex-direction: column;
    width: 350rpx;
    margin: 0 16rpx;
    line-height: 35rpx;
    margin-top: 10rpx;
}

.normal-package .package-item .item-btn {
    width: 120rpx;
    height: 60rpx;
    border-radius: 55rpx;
    box-shadow: 0 4rpx 10rpx 0 rgba(0,67,173,0.22);
    text-align: center;
    line-height: 60rpx;
    position: absolute;
    right: 10rpx;
    bottom: 20rpx;
}