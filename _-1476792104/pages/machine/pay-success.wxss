.bg-183 {
    z-index: 9;
}

.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
    z-index: 99;
}

.icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #F2F9FF;
}

.pay-success {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.pay-success image {
    width: 200rpx;
    height: 200rpx;
    margin: 217rpx 0 15rpx 0;
}

.pay-success .f26 {
    margin-top: 10rpx;
}

.pay-success .btn {
    width: 360rpx;
    height: 96rpx;
    border-radius: 55rpx;
    text-align: center;
    line-height: 96rpx;
    box-shadow: 0 4rpx 10rpx 0 rgba(0,67,173,0.22);
    margin-top: 86rpx;
}

.colse {
    width: 56rpx;
    height: 56rpx;
    float: right;
}

.lottery {
