.activity-swiper {
    height: 736rpx!important;
    margin-top: 30rpx;
    padding-bottom: 20rpx;
}

.activity-swiper .swiper {
    width: 620rpx!important;
    left: 70rpx;
    box-sizing: border-box;
    padding: 0rpx 10rpx 60rpx;
    overflow: initial;
}

.activity-swiper .swiper .swiper-item {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
    transform: scale(.95);
    transition: all .2s ease-in 0s;
    overflow: hidden;
    background: linear-gradient(135deg,#FFFFFF,#EFF3FF 30%,#FFFFFF 120%);
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 30rpx;
    position: relative;
    box-shadow: 0 6rpx 15rpx 0 rgba(209,224,252,0.94);
}

.activity-swiper .swiper .swiper-item .label {
    position: absolute;
    width: 100rpx;
    height: 82rpx;
    top: 0;
    left: 0;
}

.activity-swiper .swiper .swiper-item .swiper-top-txt {
    padding: 25rpx 0;
    position: relative;
    width: 100%;
}

.activity-swiper .swiper .swiper-item .swiper-top-txt::before {
    content: '';
    width: 500rpx;
    height: 1rpx;
    background: #D8D8D8;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
}

.activity-swiper .swiper .swiper-item .swiper-row {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 84%;
    height: 110rpx;
}

.activity-swiper .swiper .swiper-item .swiper-row .swiper-row-right {
    width: 187rpx;
    height: 58rpx;
    border-radius: 30rpx;
    border: 1rpx solid #326DFF;
    display: flex;
    justify-content: center;
    align-items: center;
}

.activity-swiper .swiper .swiper-item .swiper-row .swiper-row-right image {
    width: 29rpx;
    height: 33rpx;
    margin-right: 10rpx;
}

.activity-swiper .swiper .swiper-item .tips {
    line-height: 50rpx;
    padding-left: 40rpx;
}

.activity-swiper .swiper .swiper-item .swiper-btn {
    width: 320rpx;
    height: 68rpx;
    text-align: center;
    line-height: 68rpx;
    background: linear-gradient(45deg,#66E785,#00A98A);
    border-radius: 34rpx;
    margin-top: 30rpx;
}

.activity-swiper .swiper .swiper-item .btn-group {
    display: flex;
    width: 80%;
    justify-content: space-between;
    margin-top: 30rpx;
}

.activity-swiper .swiper .swiper-item .btn-group view {
    width: 220rpx;
    height: 68rpx;
    text-align: center;
    line-height: 68rpx;
    background: linear-gradient(45deg,#66E785,#00A98A);
    border-radius: 34rpx;
}

.activity-swiper .swiper:nth-child(3) .swiper-item .line {
    margin: 45rpx 0 24rpx 0;
}

.activity-swiper .swiper:nth-child(3) .swiper-item .row {
    margin-bottom: 15rpx;
}

.activity-swiper swiper-item.cur .swiper-item {
    transform: none;
    transition: all .2s ease-in 0s;
}

