.bg-183 {
    z-index: 9;
}

.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
    z-index: 99;
}

.icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #F2F9FF;
}

.record-evaluate {
    display: flex;
    flex-direction: column;
    padding: 80rpx 35rpx 30rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,0.08);
}

.record-evaluate .evaluate-bottom {
    display: flex;
    border-top: 1rpx solid rgba(216,216,216,0.7);
    margin-top: 15rpx;
    padding-top: 15rpx;
}

.record-evaluate .evaluate-bottom .evaluate-bottom-left {
    display: flex;
    flex-direction: column;
    line-height: 50rpx;
    padding-right: 30rpx;
    position: relative;
}

.record-evaluate .evaluate-bottom .evaluate-bottom-left::before {
    content: '';
    width: 2rpx;
    height: 174rpx;
    background: rgba(216,216,216,0.6);
    position: absolute;
    right: 0;
    margin-top: 10rpx;
}

.record-evaluate .evaluate-bottom .evaluate-bottom-left .evaluate-row {
    width: 300rpx;
    display: flex;
    justify-content: space-between;
}

.record-evaluate .evaluate-bottom .evaluate-bottom-right {
    display: flex;
    flex-direction: column;
    line-height: 50rpx;
    padding-left: 48rpx;
}

.record-evaluate .evaluate-bottom .evaluate-bottom-right .evaluate-row {
    width: 220rpx;
    display: flex;
    justify-content: space-between;
}

.record-list {
    padding: 0 30rpx;
    box-sizing: border-box;
    margin-top: 40rpx;
    min-height: 500rpx;
    position: relative;
}

.record-list .list-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 30rpx;
}

.record-list .list-top .switch-btn {
    display: flex;
    width: 220rpx;
    height: 56rpx;
    border-radius: 40rpx;
    border: 1rpx solid #bbbbbb;
}

.record-list .list-top .switch-btn .switch-item {
    position: relative;
    flex: auto;
    text-align: center;
    line-height: 56rpx;
}

.record-list .list-top .switch-btn .active {
    color: #FFFFFF;
    background: #2660E7;
    border-radius: 40rpx;
}

.latest-record {
    width: 690rpx;
    border-radius: 20rpx;
    overflow: hidden;
    box-shadow: 0 8rpx 20rpx 0 rgba(53,131,254,0.22);
    margin-bottom: 30rpx;
    background: #fff;
}

.latest-record .top {
    display: flex;
    justify-content: space-between;
    overflow: hidden;
    padding-right: 30rpx;
    height: 65rpx;
}

.latest-record .top .tag {
    background: url("http://yyb-oss.universal-space.cn/imgs/record/record_tag.png") no-repeat right;
    background-size: auto 100%;
    height: 58rpx;
    line-height: 58rpx;
    color: #004AFF;
    font-size: 24rpx;
    padding-left: 20rpx;
    padding-right: 70rpx;
}

.latest-record .top .locate {
    color: #999;
    font-size: 26rpx;
    display: flex;
    align-items: center;
}

.latest-record .top .locate image {
    width: 30rpx;
    height: 30rpx;
}

.latest-record .cont {
    padding: 18rpx 30rpx 10rpx;
    display: flex;
    align-items: center;
    width: 100%;
    box-sizing: border-box;
}

.latest-record .cont .avatar {
    width: 160rpx;
    height: 160rpx;
    border-radius: 10rpx;
    border: 5rpx solid #2660E7;
    overflow: hidden;
    margin-right: 20rpx;
}

.latest-record .cont .avatar image {
    width: 160rpx;
    height: 160rpx;
}

.latest-record .cont .info .col {
    display: flex;
    font-size: 28rpx;
    color: #666;
    align-items: center;
    line-height: 50rpx;
}

.latest-record .cont .info .col image {
    margin-right: 10rpx;
    width: 40rpx;
    height: 40rpx;
}

.latest-record .cont .info .col text {
    width: 370rpx;
}

.latest-record .color-lump-container {
    display: flex;
    flex-wrap: wrap;
    margin: 0 30rpx;
    border-top: 1rpx solid rgba(216,216,216,0.7);
    padding: 20rpx 0 30rpx;
    line-height: 55rpx;
}

.latest-record .color-lump-container view {
    width: 50%;
    display: flex;
    align-items: center;
}

.latest-record .color-lump-container view image {
    width: 160rpx;
    height: 35rpx;
    margin-right: 30rpx;
}

.latest-record .score {
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-radius: 20rpx;
    height: 110rpx;
    line-height: 110rpx;
    padding: 0 30rpx;
    font-size: 24rpx;
    color: #fff;
    background: linear-gradient(to left,#2F5FFF,#3CA6FD);
}

.latest-record .score .Merriweather {
    text-shadow: 1rpx 3rpx 10rpx rgba(0,0,0,0.3);
    font-size: 60rpx;
}