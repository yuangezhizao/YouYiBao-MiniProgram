.my-eam-wrap .intro .agree {
    padding-top: 20rpx;
    margin-top: 20rpx;
    margin-bottom: 36rpx;
    border-top: 1rpx solid #eee;
    display: flex;
    align-items: center;
}

.my-eam-wrap .intro .agree .checkbox {
    border-radius: 50%;
    width: 28rpx;
    height: 28rpx;
    border: 1rpx solid #999;
    margin-right: 5rpx;
    position: relative;
    transition: all 500ms;
}

.my-eam-wrap .intro .agree .checkbox.checked {
