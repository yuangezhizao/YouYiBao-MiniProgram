    border-color: #2660E7;
}

.my-eam-wrap .intro .agree .checkbox.checked:after {
    content: '';
    width: 15rpx;
    height: 6rpx;
    border: 1rpx solid #fff;
    border-top: 0;
    border-right: 0;
    position: absolute;
    display: block;
    left: 6rpx;
    top: 10rpx;
    transform: rotate(-45deg) scale(1);
}

