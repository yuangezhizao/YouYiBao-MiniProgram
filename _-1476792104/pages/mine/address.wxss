.bg-183 {
    z-index: 9;
}

.statusBar {
    position: relative;
    z-index: 99;
}

.icon-group,.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 64rpx;
}

.icon-group {
    position: fixed;
    left: 0;
    width: 174rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid hsla(0,0%,98%,.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: hsla(0,0%,98%,.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #f2f9ff;
}

.container1 {
    padding: 45rpx 36rpx 22rpx;
    width: 100%;
    background-color: #fff;
    box-shadow: 0rpx 10rpx 23rpx 1rpx rgba(85,145,252,.1);
    border-radius: 20rpx;
}

.bg,.container1 {
    box-sizing: border-box;
}

.bg {
    padding: 0rpx 30rpx;
}