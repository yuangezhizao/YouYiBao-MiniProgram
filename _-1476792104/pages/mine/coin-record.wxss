.bg-183 {
    z-index: 9;
}

.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
    z-index: 99;
}

.icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #FFF;
}

.white-bg {
    background: #F2F9FF;
    display: flex;
    justify-content: center;
    padding-top: 75rpx;
}

.white-bg .coin {
    width: 620rpx;
    height: 210rpx;
    background: url("http://yyb-oss.universal-space.cn/imgs/card_coin.png");
    background-size: 100% 100%;
    margin-bottom: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 55rpx;
}

.white-bg .coin .my-coin {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.white-bg .coin .recharge {
    width: 200rpx;
    height: 72rpx;
    background: linear-gradient(to right,#E8E4FF,#FFFFFF);
    border-radius: 55rpx;
    text-align: center;
    line-height: 72rpx;
    color: #5F6AF2;
}

.coin-record {
    padding: 29rpx 26rpx;
    box-sizing: border-box;
    position: relative;
    min-height: 800rpx;
}

.coin-record .record-list {
    border-bottom: 1rpx solid #E0E0E0;
    margin-bottom: 18rpx;
}

.coin-record .record-list .list-item {
    display: flex;
    justify-content: space-between;
    margin-bottom: 21rpx;
}