    height: 110rpx;
    line-height: 110rpx;
    background: linear-gradient(to right,#3ca6fd,#2f5fff);
    border: 0;
    margin: 50rpx auto;
    box-shadow: 0 8rpx 20rpx 0 rgba(53,131,254,0.22);
}

.acbs-wrap {
    padding-top: 70rpx;
}

.card-item {
    height: 420rpx;
}

.card-item.blue .card-bd {
    background-image: url('http://yyb-oss.universal-space.cn/imgs/mine/vip_store.png');
}

.card-item .card-bd {
    margin: auto;
    position: relative;
    padding: 50rpx;
    box-sizing: border-box;
    background-size: 100% 100%;
    width: 720rpx;
    height: 420rpx;
    border-radius: 20rpx;
    overflow: hidden;
    color: #fff;
    font-weight: 200;
}

.card-item .card-bd .btn {
    font-size: 26rpx;
    height: 50rpx;
    line-height: 50rpx;
    min-width: 125rpx;
}

.card-item .card-bd .name {
    display: flex;
    align-items: center;
    margin-top: 30rpx;
    width: 100%;
}

.card-item .card-bd .name image {
    width: 30rpx;
    min-width: 30rpx;
    height: 30rpx;
}

.card-item .card-bd .sub-name {
    font-size: 28rpx;
    display: flex;
    justify-content: space-between;
    position: relative;
    padding-top: 10rpx;
    margin-top: 10rpx;
}

.card-item .card-bd .sub-name:before {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    height: 2rpx;
    background: #fff;
    width: 40rpx;
}

.card-item .card-bd .card-tips {
    margin-top: 140rpx;
}

.select-list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: row;
    flex-wrap: wrap;
    margin-top: 20rpx;
}

.select-list .option-item {
    width: 320rpx;
    height: 220rpx;
    border-radius: 20rpx;
    background-color: #fff;
    border: 1rpx solid #bbb;
    box-sizing: border-box;
    margin-bottom: 32rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.select-list .option-item.active {
    box-shadow: 0 6rpx 10rpx 0 rgba(96,138,255,0.28);
    border: 4rpx solid #2660e7;
}

.select-list .option-item .money {
    display: flex;
    justify-content: center;
    align-items: flex-end;
    margin-bottom: 10rpx;
}

.select-list .option-item .money .Merriweather {
    text-shadow: 1rpx 3rpx 10rpx rgba(49,108,254,0.3);
}

.select-list .option-item .money .f26 {
    transform: translate(10rpx,-20rpx);
}

.select-list .agree {
    display: flex;
    align-items: center;
}

.select-list .agree .checkbox {
    border-radius: 50%;
    width: 28rpx;
    height: 28rpx;
    border: 1rpx solid #999;
    margin-right: 5rpx;
    position: relative;
    transition: all 500ms;
}

.select-list .agree .checkbox.checked {
    background-color: #2660E7;
    border-color: #2660E7;
}

.select-list .agree .checkbox.checked:after {
    content: '';
    width: 15rpx;
    height: 6rpx;
    border: 1rpx solid #fff;
    border-top: 0;
    border-right: 0;
    position: absolute;
    display: block;
    left: 6rpx;
    top: 10rpx;
    transform: rotate(-45deg) scale(1);
}

.open-card-record {
    text-align: center;
    position: absolute;
    bottom: 30rpx;
    left: 50%;
    transform: translateX(-50%);
}