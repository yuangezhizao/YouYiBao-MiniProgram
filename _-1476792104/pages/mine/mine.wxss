.confirm-box {
    width: 540rpx;
    height: 469rpx;
    background: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0 40rpx;
    box-sizing: border-box;
}

.confirm-box .close {
    width: 42rpx;
    height: 70rpx;
    position: absolute;
    right: 10rpx;
    top: -70rpx;
}

.confirm-box .top {
    width: 100%;
    height: 136rpx;
    position: absolute;
    top: 0;
    text-align: center;
    line-height: 136rpx;
}

.confirm-box .content {
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 130rpx 0 40rpx 0;
    text-align: center;
}

.confirm-box .btn_group_cus {
    width: 290rpx;
    height: 96rpx;
    border-radius: 55rpx;
    text-align: center;
    line-height: 96rpx;
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

.confirm-box .button_group {
    display: flex;
    justify-content: center;
    width: 100%;
}

.confirm-box .button_group .button {
    width: 210rpx;
    height: 90rpx;
    border-radius: 55rpx;
    background: linear-gradient(45deg,#9A9A9A,#575757);
    text-align: center;
    line-height: 90rpx;
    box-shadow: 0 8rpx 16rpx 0 rgba(161,161,161,0.22);
    margin: 0 15rpx;
}

.confirm-box .button_group .button:last-child {
    background: linear-gradient(45deg,#3CA6FD,#2F5FFF);
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

./components/scan.wxssrpx.tabbar_box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 98rpx;
    box-shadow: 0 -4rpx 25rpx 0 rgba(0,0,0,0.08);
}

.tabbar_box.iphoneX-height {
    padding-bottom: 68rpx;
}

.middle-wrapper {
    position: absolute;
    right: 310rpx;
    bottom: 0;
    background-color: #fff;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
}

.middle-wrapper.iphoneX-height {
    bottom: 66rpx;
}

.tabbar_nav {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 98rpx;
    position: relative;
    width: auto;
}

.tabbar_icon {
    width: 56rpx;
    height: 56rpx;
}

.special-wrapper {
    position: absolute;
    top: -50rpx;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
    background-color: #fff;
    text-align: center;
    box-sizing: border-box;
}

.special-wrapper .tabbar_icon {
    width: 120rpx;
    height: 120rpx;
}

.special-text-wrapper {
    width: 56rpx;
    height: 56rpx;
}

page {
    background: #F2F9FF;
    padding-bottom: 200rpx;
}

.white-bg {
    background: #fff;
}

.white-bg .user-info {
    display: flex;
    align-items: center;
    width: 690rpx;
    height: 260rpx;
    border-radius: 20rpx;
    margin: 0 auto;
    margin-top: 50rpx;
    background-color: #fff;
    box-shadow: 0 4rpx 20rpx 0 rgba(85,145,252,0.3);
    position: relative;
    background: url("http://yyb-oss.universal-space.cn/imgs/card_vip.png");
    background-size: 100% 100%;
}

.white-bg .user-info .user-avatar {
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
    background: #ccc;
    margin: 0 30rpx;
    border: 4rpx solid #347AFE;
    box-shadow: 0 8rpx 10rpx 0 rgba(53,131,254,0.1);
}

.white-bg .user-info .active {
    border: 4rpx solid #fff;
}

.white-bg .user-info .user-info-item {
    display: flex;
    flex-direction: column;
    width: 37%;
    line-height: 50rpx;
}

.white-bg .user-info .user-lottery {
    width: 200rpx;
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 60rpx;
    box-shadow: 0 8rpx 20rpx 0 rgba(53,131,254,0.22);
    background: linear-gradient(45deg,#3CA6FD,#2F5FFF);
    position: relative;
}

.white-bg .user-info .user-lottery image {
    width: 54rpx;
    height: 54rpx;
    margin: 0 20rpx 0 0;
}

.white-bg .user-info .user-lottery .num {
    position: absolute;
    min-width: 38rpx;
    height: 38rpx;
    border-radius: 50%;
    border: 3rpx solid #fff;
    font-size: 26rpx;
    text-align: center;
    line-height: 38rpx;
    top: 0;
    right: -15rpx;
    background: linear-gradient(135deg,#f99,#dc1b1b);
}

.white-bg .wealth {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 40rpx 0 15rpx 0;
}

.white-bg .wealth .wealth-title {
    color: #3788FE;
}

.white-bg .wealth .wealth-list {
    display: flex;
    margin: 30rpx 0;
}

.white-bg .wealth .wealth-list .wealth-list-item {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.white-bg .wealth .wealth-list .wealth-list-item image {
    width: 150rpx;
    height: 150rpx;
    margin-right: 10rpx;
}

.game-medal {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
    box-sizing: border-box;
    margin-bottom: 15rpx;
}

.game-medal .game-medal-top {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: 20rpx 0;
}

.game-medal .game-medal-bottom {
    width: 100%;
    height: 230rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0 6rpx 10rpx 4rpx #e9f1ff;
    display: flex;
    justify-content: space-around;
}

.game-medal .game-medal-bottom .medal-type {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    width: 33.333%;
}

.game-medal .game-medal-bottom .medal-type .medal-image {
    width: 112rpx;
    height: 112rpx;
    margin: 10rpx 0;
    position: relative;
}

.game-medal .game-medal-bottom .medal-type .medal-image.disabled {
    filter: grayscale(1);
}

.game-medal .game-medal-bottom .medal-type .medal-image.disabled:after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: #fff;
    opacity: .6;
}

.game-medal .game-medal-bottom .medal-type .v3 {
    background: linear-gradient(to right,#FFD74C,#FF9014);
}

.game-medal .game-medal-bottom .medal-type .v2 {
    background: linear-gradient(to right,#9EA4B4,#626880);
}

.game-medal .game-medal-bottom .medal-type .v1 {
    background: linear-gradient(to right,#C8BCC6,#A48D9F);
}

.game-medal .game-medal-bottom .medal-type .v0 {
    background: #BBBBBB;
}

.card-package {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.card-package .card-package-top {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: 20rpx 0;
}

.card-package .card-package-bottom {
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 302rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0 6rpx 10rpx 4rpx #e9f1ff;
}

.card-package .card-package-bottom .card-package-list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #E6E8F0;
    height: 110rpx;
    padding: 0 33rpx;
}

.card-package .card-package-bottom .card-package-list .list-left {
    display: flex;
    align-items: center;
}

.card-package .card-package-bottom .card-package-list .list-left image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
}

.card-package .card-package-bottom .card-package-list image {
    width: 40rpx;
    height: 40rpx;
}

.card-package .card-package-bottom .card-package-list:last-child {
    border: none;
}