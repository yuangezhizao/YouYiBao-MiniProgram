.match-dialog {
    background-color: rgba(0,0,0,0.6);
    position: fixed;
    left: 0;
    top: 0;
    width: 100vw;
    height: 100vh;
    z-index: 999;
    display: flex;
    justify-content: center;
    align-items: center;
    animation: fade both 500ms;
}

.match-dialog.large .match-dialog-inner {
    width: 650rpx;
}

.match-dialog .match-dialog-inner {
    width: 540rpx;
    border-radius: 10rpx;
    background-color: #fff;
    position: relative;
    animation: zoomIn both 200ms;
}

.match-dialog .match-dialog-inner .title {
    background-image: url("http://yyb-oss.universal-space.cn/imgs/bg_3.png");
    background-size: 100% 100%;
    height: 150rpx;
    line-height: 150rpx;
    color: #fff;
    font-size: 34rpx;
    text-align: center;
    overflow: hidden;
}

.match-dialog .match-dialog-inner .close {
    width: 42rpx;
    height: 70rpx;
    position: absolute;
    right: 10rpx;
    top: -70rpx;
}

.match-dialog .match-dialog-inner .match-dialog-body {
    padding: 0 30rpx 59rpx 30rpx;
    color: #2660e7;
    font-size: 34rpx;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-primary {
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
    background: linear-gradient(to right,#3CA6FD,#2F5FFF);
    height: 96rpx;
    line-height: 96rpx;
    min-width: 290rpx;
    margin-top: 50rpx;
    border: 0;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group {
    width: 100%;
    justify-content: space-between;
    display: flex;
    box-sizing: border-box;
    padding: 0 30rpx;
    margin-top: 50rpx;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group .btn {
    min-width: 200rpx;
    width: auto;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 55rpx;
    border: 0;
    margin: 0;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group .btn.btn-primary {
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
    background: linear-gradient(to right,#3CA6FD,#2F5FFF);
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group .btn.btn-disabled {
    background: linear-gradient(to right,#9a9a9a,#575757);
    box-shadow: 0 8rpx 16rpx 0 rgba(161,161,161,0.22);
}

@-webkit-keyframes fade {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes fade {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@-webkit-keyframes zoomIn {
    from {
        opacity: 0;
        transform: scale(1.5);
    }

    to {
        opacity: 1;
        transform: none;
    }
}

@keyframes zoomIn {
    from {
        opacity: 0;
        transform: scale(1.5);
    }

    to {
        opacity: 1;
        transform: none;
    }
}

.match-status-bar {
    position: relative;
    z-index: 99;
}

.match-status-bar .statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
}

.match-status-bar .statusBar .icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999;
}

.match-status-bar .statusBar .icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.match-status-bar .statusBar .icon-group .left {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

.btn {
    position: relative;
    display: inline-block;
    height: 88rpx;
    padding: 0;
    font-size: 32rpx;
    line-height: 84rpx;
    text-align: center;
    vertical-align: middle;
    box-sizing: border-box;
    border-radius: 10rpx;
    -webkit-appearance: none;
    -webkit-text-size-adjust: 100%;
    transition: all 500ms;
}

.btn::before {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 100%;
    height: 100%;
    background-color: #000;
    border: inherit;
    border-color: #000;
    border-radius: inherit;
    content: ' ';
    opacity: 0;
    transform: translate(-50%,-50%);
}

.btn::after {
    border-width: 0;
}

.btn:active {
    transform: translateY(.1em);
}

.btn-active::before {
    opacity: .15;
}

.btn-unclickable::after {
    display: none;
}

.btn-default {
    color: #bbb;
    background-color: #fff;
    border: 1rpx solid #fff;
}

.btn-primary {
    color: #fff;
    background-color: #2660E7;
    border: 1rpx solid #2660E7;
}

.btn-danger {
    color: #fff;
    background-color: #E13434;
    border: 1rpx solid #E13434;
}

.btn-warning {
    color: #fff;
    background-color: #F6C96A;
    border: 1rpx solid #F6C96A;
}

.btn-success {
    color: #fff;
    background-color: #5BE89B;
    border: 1rpx solid #5BE89B;
}

.btn-plain {
    background-color: #fff;
}

.btn-plain.btn-primary {
    color: #2660E7;
}

.btn-plain.btn-black {
    border: 1rpx solid #999;
}

.btn-plain.btn-danger {
    color: #E13434;
}

.btn-plain.btn-warning {
    color: #F6C96A;
}

.btn-large {
    width: 100%;
    height: 100rpx;
    line-height: 96rpx;
}

.btn-normal {
    padding: 0 30rpx;
    font-size: 28rpx;
}

.btn-small {
    height: 66rpx;
    min-width: 120rpx;
    padding: 0 20rpx;
    font-size: 26rpx;
    line-height: 66rpx;
}

.btn-mini {
    display: inline-block;
    width: 100rpx;
    height: 44rpx;
    font-size: 20rpx;
    line-height: 40rpx;
}

.btn-mini+.btn-mini {
    margin-left: 10rpx;
}

.btn-block {
    display: block;
    width: 100%;
}

.btn-round {
    border-radius: 2em;
}

.btn-square {
    border-radius: 0;
}

.btn-disabled {
    color: #fff;
    background-color: #bbb;
}

page {
    background: #fff;
}

.set-meal {
    background: linear-gradient(to right,#3CA6FD,#2F5FFF);
    width: 320rpx;
    height: 190rpx;
    border-radius: 20rpx;
    box-shadow: 0 8rpx 20rpx 0 rgba(53,131,254,0.22);
    justify-content: center;
    align-items: center;
    color: #fff;
    margin: 74rpx auto auto;
    display: flex;
    flex-direction: column;
    line-height: 1;
}

.set-meal .num {
    display: flex;
    justify-content: center;
    align-items: flex-end;
    margin-bottom: 10rpx;
}

.set-meal .num .Merriweather {
    text-shadow: 1rpx 3rpx 10rpx rgba(49,108,254,0.3);
}

.set-meal .num .f26 {
    transform: translate(10rpx,-20rpx);
}

.form {
    padding: 98rpx;
}

.form .field {
    border-bottom: 1rpx solid #d8d8d8;
    position: relative;
    height: 117rpx;
    display: flex;
    align-items: flex-end;
    box-sizing: border-box;
    padding-bottom: 20rpx;
}

.form .field input {
    color: #bbb;
    font-size: 30rpx;
}

.form .field .arrow {
    width: 22rpx;
    height: 22rpx;
    display: flex;
    transform: translate(-30rpx,-10rpx);
}

.form .field .arrow image {
    width: 22rpx;
    height: 22rpx;
}

.form .field .btn {
    width: 200rpx;
    height: 72rpx;
    line-height: 72rpx;
    font-size: 30rpx;
}

.form .field button[disabled][type="default"],.form .field button[disabled]:not([type]) {
    border: 1rpx solid #BFBFBF;
    background: none;
    color: #BFBFBF;
}

.form .submit {
    width: 480rpx;
    height: 108rpx;
    line-height: 108rpx;
    border: 0;
    margin: auto;
    margin-top: 80rpx;
    display: block;
}

.form button[disabled][type="default"],.form button[disabled]:not([type]) {
    background: linear-gradient(to right,#9A9A9A,#575757);
    color: #fff;
}