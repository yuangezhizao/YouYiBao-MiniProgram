.confirm-box {
    width: 540rpx;
    height: 469rpx;
    background: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0 40rpx;
    box-sizing: border-box;
}

.confirm-box .close {
    width: 42rpx;
    height: 70rpx;
    position: absolute;
    right: 10rpx;
    top: -70rpx;
}

.confirm-box .top {
    width: 100%;
    height: 136rpx;
    position: absolute;
    top: 0;
    text-align: center;
    line-height: 136rpx;
}

.confirm-box .content {
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 130rpx 0 40rpx 0;
    text-align: center;
}

.confirm-box .btn_group_cus {
    width: 290rpx;
    height: 96rpx;
    border-radius: 55rpx;
    text-align: center;
    line-height: 96rpx;
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

.confirm-box .button_group {
    display: flex;
    justify-content: center;
    width: 100%;
}

.confirm-box .button_group .button {
    width: 210rpx;
    height: 90rpx;
    border-radius: 55rpx;
    background: linear-gradient(45deg,#9A9A9A,#575757);
    text-align: center;
    line-height: 90rpx;
    box-shadow: 0 8rpx 16rpx 0 rgba(161,161,161,0.22);
    margin: 0 15rpx;
}

.confirm-box .button_group .button:last-child {
    background: linear-gradient(45deg,#3CA6FD,#2F5FFF);
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

.bg-183 {
    z-index: 9;
}

.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
    z-index: 99;
}

.icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

.login {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.login .login-logo {
    width: 200rpx;
    height: 200rpx;
    background: #ccc;
    border-radius: 50%;
    margin: 168rpx 0 90rpx 0;
}

.login .quick-login {
    width: 480rpx;
    height: 110rpx;
    border-radius: 60rpx;
    text-align: center;
    box-shadow: 0 8rpx 20rpx 0 rgba(53,131,254,0.22);
}

.login .quick-login .txt {
    line-height: 110rpx;
}

.login .login-agreement {
    margin: 30rpx 0 43rpx 0;
}

.btn_group_cus {
    margin-top: 47rpx;
}

.btn_group_cus .ib {
    width: 206rpx;
    height: 69rpx;
    line-height: 69rpx;
}

.btn_group_cus .ib view {
    width: 206rpx;
    height: 69rpx;
    line-height: 69rpx;
}

.btn_group_cus .no view {
    color: #0570bc;
}

.btn_group_cus .yes {
    margin-left: 75rpx;
}

.btn_group_cus .yes view {
    color: #ffffff;
}