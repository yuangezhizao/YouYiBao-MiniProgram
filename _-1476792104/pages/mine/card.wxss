}

.card-item {
    height: 400rpx;
}

.card-item.blue .card-bd {
    background-image: url("http://yyb-oss.universal-space.cn/imgs/mine/vip_details_card.png");
}

.card-item .card-bd {
    margin: auto;
    position: relative;
    padding: 50rpx 60rpx;
    box-sizing: border-box;
    background-size: 100% 100%;
    width: 750rpx;
    height: 420rpx;
    border-radius: 20rpx;
    overflow: hidden;
    color: #fff;
    font-weight: 200;
}

.card-item .card-bd.disabled {
    filter: grayscale(1);
}

.card-item .card-bd.disabled:after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: #fff;
    opacity: .4;
}

.card-item .card-bd .btn {
    font-size: 26rpx;
    height: 50rpx;
    line-height: 50rpx;
    min-width: 125rpx;
}

.card-item .card-bd .name {
    display: flex;
    align-items: center;
    margin-top: 30rpx;
    justify-content: space-between;
    height: 53rpx;
}

.card-item .card-bd .sub-name {
    font-size: 28rpx;
    display: flex;
    justify-content: space-between;
    position: relative;
    padding-top: 10rpx;
    margin-top: 10rpx;
}

.card-item .card-bd .sub-name:before {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    height: 2rpx;
    background: #fff;
    width: 40rpx;
}

.card-item .card-bd .expired {
    position: absolute;
    right: 65rpx;
    top: 145rpx;
}

.card-item .card-bd .card-num {
    font-size: 30rpx;
    position: absolute;
    right: 50rpx;
    left: 50rpx;
    bottom: 60rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 66rpx;
}

.card-item .card-bd .card-num .btn-unbind {
    width: 200rpx;
    height: 66rpx;
    line-height: 66rpx;
    border-radius: 33rpx;
    background: linear-gradient(to right,#e4efff,#fff);
    color: #2660E7;
    z-index: 99;
}

.card-detail {
