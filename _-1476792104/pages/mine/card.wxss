.confirm-box {
    width: 540rpx;
    height: 469rpx;
    background: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0 40rpx;
    box-sizing: border-box;
}

.confirm-box .close {
    width: 42rpx;
    height: 70rpx;
    position: absolute;
    right: 10rpx;
    top: -70rpx;
}

.confirm-box .top {
    width: 100%;
    height: 136rpx;
    position: absolute;
    top: 0;
    text-align: center;
    line-height: 136rpx;
}

.confirm-box .content {
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 130rpx 0 40rpx 0;
    text-align: center;
}

.confirm-box .btn_group_cus {
    width: 290rpx;
    height: 96rpx;
    border-radius: 55rpx;
    text-align: center;
    line-height: 96rpx;
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

.confirm-box .button_group {
    display: flex;
    justify-content: center;
    width: 100%;
}

.confirm-box .button_group .button {
    width: 210rpx;
    height: 90rpx;
    border-radius: 55rpx;
    background: linear-gradient(45deg,#9A9A9A,#575757);
    text-align: center;
    line-height: 90rpx;
    box-shadow: 0 8rpx 16rpx 0 rgba(161,161,161,0.22);
    margin: 0 15rpx;
}

.confirm-box .button_group .button:last-child {
    background: linear-gradient(45deg,#3CA6FD,#2F5FFF);
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
}

.match-dialog {
    background-color: rgba(0,0,0,0.6);
    position: fixed;
    left: 0;
    top: 0;
    width: 100vw;
    height: 100vh;
    z-index: 999;
    display: flex;
    justify-content: center;
    align-items: center;
    animation: fade both 500ms;
}

.match-dialog.large .match-dialog-inner {
    width: 650rpx;
}

.match-dialog .match-dialog-inner {
    width: 540rpx;
    border-radius: 10rpx;
    background-color: #fff;
    position: relative;
    animation: zoomIn both 200ms;
}

.match-dialog .match-dialog-inner .title {
    background-image: url("http://yyb-oss.universal-space.cn/imgs/bg_3.png");
    background-size: 100% 100%;
    height: 150rpx;
    line-height: 150rpx;
    color: #fff;
    font-size: 34rpx;
    text-align: center;
    overflow: hidden;
}

.match-dialog .match-dialog-inner .close {
    width: 42rpx;
    height: 70rpx;
    position: absolute;
    right: 10rpx;
    top: -70rpx;
}

.match-dialog .match-dialog-inner .match-dialog-body {
    padding: 0 30rpx 59rpx 30rpx;
    color: #2660e7;
    font-size: 34rpx;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-primary {
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
    background: linear-gradient(to right,#3CA6FD,#2F5FFF);
    height: 96rpx;
    line-height: 96rpx;
    min-width: 290rpx;
    margin-top: 50rpx;
    border: 0;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group {
    width: 100%;
    justify-content: space-between;
    display: flex;
    box-sizing: border-box;
    padding: 0 30rpx;
    margin-top: 50rpx;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group .btn {
    min-width: 200rpx;
    width: auto;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 55rpx;
    border: 0;
    margin: 0;
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group .btn.btn-primary {
    box-shadow: 0 8rpx 16rpx 0 rgba(53,131,254,0.22);
    background: linear-gradient(to right,#3CA6FD,#2F5FFF);
}

.match-dialog .match-dialog-inner .match-dialog-body .btn-group .btn.btn-disabled {
    background: linear-gradient(to right,#9a9a9a,#575757);
    box-shadow: 0 8rpx 16rpx 0 rgba(161,161,161,0.22);
}

@-webkit-keyframes fade {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes fade {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@-webkit-keyframes zoomIn {
    from {
        opacity: 0;
        transform: scale(1.5);
    }

    to {
        opacity: 1;
        transform: none;
    }
}

@keyframes zoomIn {
    from {
        opacity: 0;
        transform: scale(1.5);
    }

    to {
        opacity: 1;
        transform: none;
    }
}

.match-status-bar {
