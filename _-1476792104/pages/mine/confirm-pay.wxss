.bg-183 {
    z-index: 9;
}

.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 64rpx;
    z-index: 99;
}

.icon-group {
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    left: 0;
    width: 174rpx;
    height: 64rpx;
    background-color: rgba(0,0,0,0.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid rgba(250,250,250,0.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: rgba(250,250,250,0.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #F2F9FF;
}

.price {
    display: flex;
    justify-content: center;
    align-items: baseline;
    padding: 93rpx 0 62rpx 0;
    background: #fff;
}

.package-info {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 35rpx;
    border-radius: 20rpx;
    margin-top: -10rpx;
    background: linear-gradient(to top,rgba(255,255,255,0.1),#e3ecff);
}

.package-info .package-detail {
    display: flex;
    flex-direction: column;
    width: 690rpx;
    height: 319rpx;
    border-radius: 10rpx;
    box-shadow: 0 4rpx 30rpx 0 rgba(85,145,252,0.3);
    padding: 46rpx 55rpx;
    box-sizing: border-box;
}

.package-info .package-detail .row {
    display: flex;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.confirm-pay {
    width: 340rpx;
    height: 96rpx;
    border-radius: 55rpx;
    box-shadow: 0 4rpx 10rpx 0 rgba(0,67,173,0.22);
    text-align: center;
    line-height: 96rpx;
    margin-top: 58rpx;
}