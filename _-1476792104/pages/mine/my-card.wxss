.card-list {
    background: linear-gradient(to bottom,#E3ECFF,#F2F9FF);
    border-radius: 30rpx 30rpx 0 0;
    position: relative;
    top: -30rpx;
}

.card-list .card-item {
    height: 190rpx;
}

.card-list .card-item:nth-child(n+1) .card-bd {
    background-image: url("http://yyb-oss.universal-space.cn/imgs/mine/card_blue.png");
}

.card-list .card-item:nth-child(2n+2) .card-bd {
    background-image: url("http://yyb-oss.universal-space.cn/imgs/mine/card_gold.png");
}

.card-list .card-item .card-bd,.card-list .card-item .card-no {
    margin: auto;
    position: relative;
    padding: 50rpx;
    box-sizing: border-box;
    background-size: 100% 100%;
    width: 720rpx;
    height: 420rpx;
    border-radius: 20rpx;
    overflow: hidden;
    color: #fff;
    font-weight: 200;
    background-image: url("http://yyb-oss.universal-space.cn/imgs/mine/card_no_1.png");
}

.card-list .card-item .card-bd .btn,.card-list .card-item .card-no .btn {
    font-size: 26rpx;
    height: 50rpx;
    line-height: 50rpx;
    min-width: 125rpx;
}

.card-list .card-item .card-bd .name,.card-list .card-item .card-no .name {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.card-list .card-item .card-bd .sub-name,.card-list .card-item .card-no .sub-name {
    font-size: 30rpx;
    display: flex;
    justify-content: space-between;
}

.card-list .card-item .card-bd .mobile,.card-list .card-item .card-no .mobile {
    position: absolute;
    bottom: 65rpx;
    right: 45rpx;
}

.card-list .card-item .card-bd .card-num,.card-list .card-item .card-no .card-num {
    font-size: 30rpx;
    position: absolute;
    right: 50rpx;
    bottom: 50rpx;
}

.card-list .dis-card-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    line-height: 70rpx;
}

.card-list .dis-card-item image {
    width: 127rpx;
    height: 88rpx;
    margin: 150rpx auto 30rpx;
}

.my-card-top {
    background-color: #fff;
    box-sizing: border-box;
    padding: 100rpx 30rpx 60rpx 30rpx;
    display: flex;
    justify-content: space-between;
    align-self: center;
}

.my-card-top .btn-add {
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
    background: url("http://yyb-oss.universal-space.cn/imgs/mine/icon_card_add to.png");
    background-size: 100% 100%;
}

.my-card-top .left {
    display: flex;
    align-items: flex-end;
}

