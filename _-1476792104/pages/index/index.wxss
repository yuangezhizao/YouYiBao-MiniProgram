./components/scan.wxssrpx.tabbar_box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 98rpx;
    box-shadow: 0rpx -4rpx 25rpx 0rpx rgba(0,0,0,.08);
}

.tabbar_box.iphoneX-height {
    padding-bottom: 68rpx;
}

.middle-wrapper {
    position: absolute;
    right: 310rpx;
    bottom: 0;
    background-color: #fff;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
}

.middle-wrapper.iphoneX-height {
    bottom: 66rpx;
}

.tabbar_nav {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 98rpx;
    position: relative;
    width: auto;
}

.tabbar_icon {
    width: 56rpx;
    height: 56rpx;
}

.special-wrapper {
    position: absolute;
    top: -50rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
    background-color: #fff;
    text-align: center;
    box-sizing: border-box;
}

.special-wrapper,.special-wrapper .tabbar_icon {
    width: 120rpx;
    height: 120rpx;
}

.special-text-wrapper {
    width: 56rpx;
    height: 56rpx;
}

page {
    background: #fff;
}

.location {
    justify-content: space-between;
    margin-top: 20rpx;
    position: relative;
    padding: 0 40rpx;
}

.location,.location .location-left {
    display: flex;
    align-items: center;
}

.location .location-left .icon {
    width: 24rpx;
    height: 28rpx;
    margin-right: 10rpx;
}

.swiper {
    width: 680rpx;
    height: 440rpx;
    margin: 30rpx auto 0;
}

.swiper,.swiper .swiper-item {
    border-radius: 10rpx;
    overflow: hidden;
    transform: translateY(0);
}

.swiper .swiper-item {
    display: flex;
    justify-content: center;
}

.swiper .swiper-item .swiper-img {
    width: 680rpx;
    height: 380rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.swiper.square-dot .wx-swiper-dot {
    background-color: #3ba3fd;
    width: 16rpx;
    height: 16rpx;
    border-radius: 20rpx;
    margin: 0 8rpx!important;
}

.swiper.square-dot .wx-swiper-dot.wx-swiper-dot-active {
    opacity: 1;
    width: 40rpx;
}

.classify {
    display: flex;
    justify-content: center;
    padding-bottom: 50rpx;
}

.classify .classify-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 30rpx;
}

.classify .classify-item image {
    width: 80rpx;
    height: 80rpx;
}

.classify .classify-item text {
    font-size: 28rpx;
    color: #bdbdbd;
}

.type {
    height: 180rpx;
    line-height: 320rpx;
    white-space: nowrap;
    background: #f2f9ff;
}

.type .type-1 {
    width: 238rpx;
    height: 150rpx;
    margin-left: 10rpx;
}

.match {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
}

.match .match-top {
    display: flex;
    justify-content: space-between;
    margin: 40rpx 0;
}

.match .match-top .match-top-left {
    width: 161rpx;
    height: 58rpx;
}

.match .match-top text {
    line-height: 70rpx;
}

.match .match-banner {
    width: 100%;
    height: 350rpx;
    position: relative;
    border-radius: 16rpx;
    overflow: hidden;
    margin-bottom: 36rpx;
}

.match .match-banner.disabled {
    filter: grayscale(1);
}

.match .match-banner .status {
    position: absolute;
    left: -10rpx;
    top: -15rpx;
    width: 125rpx;
    height: 106rpx;
}

.match .match-banner .status image {
    width: 125rpx;
    height: 106rpx;
}

.match .match-banner .match-banner-title {
    width: 100%;
    position: absolute;
    bottom: 0;
    padding: 20rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: linear-gradient(0deg,rgba(34,34,34,.35),rgba(232,241,254,0));
}

.match .match-banner .match-banner-title .title {
    max-width: 438rpx;
}

.hot-game {
    display: flex;
    flex-direction: column;
    background: #f2f9ff;
    width: 100%;
    height: 900rpx;
    position: relative;
    background: url("http://yyb-oss.universal-space.cn/imgs/hot-bg.png");
    background-size: 100% 100%;
}

.hot-game .game-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 15rpx 30rpx;
}

.hot-game .game-top .game-top-left {
    width: 240rpx;
    height: 58rpx;
}

.hot-game .game-top text {
    line-height: 70rpx;
}

.hot-game .game-content {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.hot-game .game-content .game-avatar {
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    margin-bottom: 90rpx;
}

.hot-game .game-content .icon {
    width: 23rpx;
    height: 24rpx;
    margin-right: 5rpx;
}

.hot-game .game-content:nth-child(2) {
    position: absolute;
    top: 130rpx;
    left: 311rpx;
}

.hot-game .game-content:nth-child(3) {
    position: absolute;
    top: 273rpx;
    left: 97rpx;
    max-width: 128rpx;
    white-space: nowrap;
}

.hot-game .game-content:nth-child(3) .game-avatar {
    width: 128rpx;
    height: 128rpx;
}

.hot-game .game-content:nth-child(3) .game-name,.hot-game .game-content:nth-child(3) .icon {
    margin-left: 50rpx;
}

.hot-game .game-content:nth-child(4) {
    position: absolute;
    top: 412rpx;
    left: 524rpx;
}

.hot-game .game-content:nth-child(4) .game-avatar {
    width: 125rpx;
    height: 125rpx;
}

.hot-game .game-content:nth-child(4) .game-name,.hot-game .game-content:nth-child(4) .icon {
    margin-left: -30rpx;
}