.star-list {
    display: flex;
}

.star-list image {
    width: 44rpx;
    height: 44rpx;
    margin: 0 5rpx;
}

.bg-183 {
    z-index: 9;
}

.statusBar {
    position: relative;
    z-index: 99;
}

.icon-group,.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 64rpx;
}

.icon-group {
    position: fixed;
    left: 0;
    width: 174rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid hsla(0,0%,98%,.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: hsla(0,0%,98%,.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #f2f9ff;
}

.trophy-list {
    display: flex;
    flex-direction: column;
    padding: 120rpx 30rpx 0;
}

.trophy-list .trophy-item {
    width: 100%;
    min-height: 260rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0rpx 6rpx 10rpx 0rpx #e9f1ff;
    margin-bottom: 30rpx;
    padding: 40rpx 35rpx 10rpx;
    box-sizing: border-box;
}

.trophy-list .trophy-item .item-body {
    display: flex;
    border-bottom: 2rpx solid hsla(0,0%,84.7%,.7);
    padding-bottom: 10rpx;
    position: relative;
    margin-bottom: 15rpx;
}

.trophy-list .trophy-item .item-body.lock .trophy {
    position: relative;
}

.trophy-list .trophy-item .item-body.lock .trophy:after {
    content: "";
    position: absolute;
    width: 100%;
    height: 100%;
    display: block;
    left: 0;
    top: 0;
    background: url("http://yyb-oss.universal-space.cn/imgs/record/icon_record_lock.png") no-repeat 50% rgba(0,0,0,.5);
    background-size: 80rpx 80rpx;
    z-index: 2;
}

.trophy-list .trophy-item .item-body .trophy {
    width: 130rpx;
    min-width: 130rpx;
    height: 130rpx;
    margin-right: 10rpx;
}

.trophy-list .trophy-item .item-body .trophy-name {
    display: flex;
    flex-direction: column;
    justify-content: center;
    max-width: 500rpx;
}

.trophy-list .trophy-item .item-body .trophy-name .star-list image {
    width: 35rpx;
    height: 35rpx;
    margin: 0 5rpx;
}

.trophy-list .trophy-bg {
    position: fixed;
    right: 0;
    bottom: 0;
    width: 487rpx;
    height: 522rpx;
    z-index: -1;
}