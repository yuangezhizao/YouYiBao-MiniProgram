.custom-progress-warp {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    padding-top: 20rpx;
}

.progress-out {
    width: 370rpx;
    height: 4rpx;
    background: #ebf0fd;
    border-radius: 5rpx;
    margin-top: 7rpx;
    position: relative;
}

.progress-out .days {
    position: absolute;
    top: -30rpx;
    z-index: 99;
    width: 60rpx;
    height: 25rpx;
    background: #3270fe;
    line-height: 25rpx;
    border-radius: 6rpx;
}

.progress-in {
    height: 100%;
    background-color: #3270fe;
}

.bg-183 {
    z-index: 9;
}

.statusBar {
    position: relative;
    z-index: 99;
}

.icon-group,.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 64rpx;
}

.icon-group {
    position: fixed;
    left: 0;
    width: 174rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid hsla(0,0%,98%,.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: hsla(0,0%,98%,.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #f2f9ff;
}

.wacca-banner {
    width: 100%;
    height: 375rpx;
    background: #fff;
    border-radius: 20rpx;
    overflow: hidden;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,.08);
}

.wacca-banner image {
    width: 100%;
    height: 100%;
}

.wacca-title {
    width: 100%;
    height: 120rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0rpx 16rpx 16rpx 0rpx rgba(85,145,252,.08);
    margin: 25rpx 0 20rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    justify-content: space-between;
}

.wacca-title,.wacca-title .title-right {
    display: flex;
    align-items: center;
}

.wacca-title .title-right image {
    width: 20rpx;
    height: 25rpx;
    margin-right: 10rpx;
}

.user-info-container {
    width: 100%;
    min-height: 246rpx;
    background: #fff;
    margin: 35rpx auto;
    box-shadow: 0rpx 6rpx 10rpx 4rpx #e9f1ff;
    display: flex;
    flex-direction: column;
}

.user-info-container .user-info {
    display: flex;
    margin-bottom: 28rpx;
}

.user-info-container .user-info .user-info-left {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 24rpx 24rpx 0;
}

.user-info-container .user-info .user-info-left .avatar {
    width: 120rpx;
    height: 120rpx;
    box-sizing: border-box;
    padding: 5rpx;
    border-radius: 50%;
    background-image: linear-gradient(135deg,#46d9f5,#3450ec);
    box-shadow: 0rpx 6rpx 8rpx 0rpx rgba(136,172,255,.43);
    margin-bottom: 15rpx;
}

.user-info-container .user-info .user-info-left .avatar image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.user-info-container .user-info .user-info-left .label {
    width: 150rpx;
    height: 44rpx;
}

.user-info-container .user-info .user-info-right {
    display: flex;
    flex-direction: column;
    width: 475rpx;
}

.user-info-container .user-info .user-info-right .nickname-wrap {
    margin: 35rpx 0 16rpx;
    display: flex;
    align-items: center;
}

.user-info-container .user-info .user-info-right .nickname-wrap .nickname {
    max-width: 170rpx;
}

.user-info-container .user-info .user-info-right .nickname-wrap .user-title {
    height: 50rpx;
    background: linear-gradient(90deg,#5660f0,#7e8dff);
    border-radius: 35rpx;
    line-height: 50rpx;
    padding: 0 25rpx;
    margin-left: 10rpx;
    max-width: 160rpx;
}

.user-info-container .user-info .user-info-right .progress-container {
    display: flex;
    flex-direction: column;
}

.record-list {
    padding: 0 30rpx;
    box-sizing: border-box;
    margin-top: 40rpx;
}

.record-list .list-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 30rpx;
}

.latest-record {
    width: 690rpx;
    border-radius: 20rpx;
    overflow: hidden;
    box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(53,131,254,.22);
    margin-bottom: 30rpx;
    background: linear-gradient(45deg,#2f5fff,#3ca6fd);
}

.latest-record .top {
    display: flex;
    justify-content: space-between;
    overflow: hidden;
    padding-right: 30rpx;
    height: 65rpx;
}

.latest-record .top .tag {
    background: url("http://yyb-oss.universal-space.cn/imgs/record/record_tag.png") no-repeat 100%;
    background-size: auto 100%;
    height: 58rpx;
    line-height: 58rpx;
    color: #004aff;
    font-size: 24rpx;
    padding-left: 20rpx;
    padding-right: 70rpx;
}

.latest-record .top .locate {
    font-size: 26rpx;
    display: flex;
    align-items: center;
}

.latest-record .top .locate image {
    width: 30rpx;
    height: 30rpx;
}

.latest-record .cont {
    padding: 18rpx 30rpx 10rpx;
    display: flex;
    align-items: center;
    width: 100%;
    box-sizing: border-box;
}

.latest-record .cont .avatar {
    width: 160rpx;
    height: 160rpx;
    border-radius: 10rpx;
    border: 5rpx solid #fff;
    overflow: hidden;
    margin-right: 20rpx;
}

.latest-record .cont .avatar image {
    width: 160rpx;
    height: 160rpx;
}

.latest-record .cont .info .col {
    display: flex;
    font-size: 28rpx;
    align-items: center;
    line-height: 50rpx;
}

.latest-record .cont .info .col image {
    margin-right: 10rpx;
    width: 40rpx;
    height: 40rpx;
}

.latest-record .cont .info .col text {
    width: 370rpx;
}

.latest-record .color-lump-container {
    display: flex;
    flex-wrap: wrap;
    margin: 0 30rpx;
    border-top: 1rpx solid hsla(0,0%,84.7%,.7);
    padding: 20rpx 0 30rpx;
    line-height: 55rpx;
}

.latest-record .color-lump-container view {
    width: 50%;
    display: flex;
    align-items: center;
}

.latest-record .color-lump-container view image {
    width: 160rpx;
    height: 35rpx;
    margin-right: 30rpx;
}

.latest-record .score {
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-radius: 20rpx;
    height: 110rpx;
    line-height: 110rpx;
    padding: 0 30rpx;
    font-size: 24rpx;
    background: #fff;
}

.latest-record .score .Merriweather {
    text-shadow: 1rpx 3rpx 10rpx rgba(0,0,0,.3);
    font-size: 60rpx;
}

.new-found {
    padding: 0 30rpx;
    box-sizing: border-box;
    margin-bottom: 40rpx;
}

.new-found .title {
    margin-bottom: 20rpx;
}

.new-found .title image {
    width: 60rpx;
    height: 38rpx;
    vertical-align: middle;
    margin-left: 10rpx;
}

.new-found .new-found-container {
    background: #fff;
    border-radius: 18rpx;
    width: 100%;
    min-height: 120rpx;
    padding: 0 34rpx;
    box-sizing: border-box;
    box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(17,85,233,.22);
}

.new-found .new-found-container .row {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 120rpx;
    border-bottom: 1rpx solid hsla(0,0%,84.7%,.7);
}

.new-found .new-found-container .row .row-right {
    max-width: 460rpx;
}

.new-found .new-found-container .row .row-right scroll-view {
    white-space: nowrap;
}

.new-found .new-found-container .row .row-right scroll-view image {
    width: 80rpx;
    height: 80rpx;
    margin-left: 28rpx;
}

.new-found .new-found-container .row .row-right scroll-view .label {
    min-width: 140rpx;
    max-width: 250rpx;
    height: 52rpx;
    background: #dce9fd;
    border-radius: 4rpx;
    line-height: 52rpx;
    margin-left: 28rpx;
    display: inline-block;
    padding: 0 20rpx;
}

.new-found .new-found-container .row:last-child {
    border: none;
}

.statis-rank {
    padding: 0 30rpx;
    box-sizing: border-box;
}

.statis-rank .switch-btn {
    display: flex;
    width: 580rpx;
    height: 72rpx;
    border-radius: 40rpx;
    border: 1rpx solid #bbb;
    margin: 30rpx auto 34rpx;
}

.statis-rank .switch-btn .switch-item {
    position: relative;
    text-align: center;
    line-height: 72rpx;
    flex: 1;
}

.statis-rank .switch-btn .active {
    color: #fff;
    background: #2660e7;
    border-radius: 40rpx;
}

.statis-rank .rank {
    width: 690rpx;
    height: 220rpx;
    border-radius: 20rpx;
    background: #fff;
    margin: 10rpx 0 20rpx;
    display: flex;
    align-items: center;
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(17,85,233,.1);
    position: relative;
}

.statis-rank .rank .rank-left {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 15rpx 0 30rpx;
}

.statis-rank .rank .rank-left .Merriweather {
    width: 85rpx;
    height: 85rpx;
    border: 4rpx solid #bbb;
    border-radius: 50%;
    text-align: center;
    line-height: 85rpx;
}

.statis-rank .rank .rank-right {
    display: flex;
    flex-direction: column;
    width: 535rpx;
}

.statis-rank .rank .rank-right .rank-right-top {
    display: flex;
    align-items: center;
}

.statis-rank .rank .rank-right .rank-right-top image {
    width: 80rpx;
    height: 80rpx;
    min-width: 80rpx;
    border: 4rpx solid #fff;
    border-radius: 50%;
    margin: 20rpx;
    box-shadow: 0rpx 8rpx 10rpx 0rpx rgba(53,131,254,.1);
}

.statis-rank .rank .rank-right .rank-right-top .user-name {
    width: 170rpx;
}

.statis-rank .rank .rank-right .rank-right-top .score {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 350rpx;
}

.statis-rank .rank .rank-right .rank-right-bottom {
    display: flex;
    border-top: 1rpx solid #d8d8d8;
    justify-content: space-between;
}

.statis-rank .rank .rank-right .rank-right-bottom .icon-item {
    display: flex;
    align-items: center;
    margin-right: 13rpx;
    margin-top: 15rpx;
}

.statis-rank .rank .rank-right .rank-right-bottom .icon-item image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 5rpx;
}

.btn-group {
    display: flex;
    justify-content: space-around;
    padding: 50rpx 30rpx 70rpx;
    box-sizing: border-box;
}

.btn-group .btn {
    width: 200rpx;
    height: 80rpx;
    background: linear-gradient(90deg,#3ca6fd,#2f5fff);
    border-radius: 55rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(53,131,254,.22);
}

.btn-group .btn:nth-child(2) {
    background: linear-gradient(90deg,#5be89b,#2eccbd);
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(18,179,159,.2);
}