.bg-183 {
    z-index: 9;
}

.statusBar {
    position: relative;
    z-index: 99;
}

.icon-group,.statusBar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 64rpx;
}

.icon-group {
    position: fixed;
    left: 0;
    width: 174rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 35rpx;
    margin-left: 30rpx;
    border: 1rpx solid hsla(0,0%,98%,.3);
    z-index: 9999!important;
}

.icon-group .line {
    width: 3rpx;
    height: 32rpx;
    background: hsla(0,0%,98%,.3);
}

.icon-group .icon {
    width: 38rpx;
    height: 38rpx;
    padding: 10rpx 20rpx;
}

page {
    background: #f2f9ff;
}

.switch-btn {
    display: flex;
    width: 580rpx;
    height: 72rpx;
    border-radius: 40rpx;
    border: 1rpx solid #bbb;
    margin: 100rpx auto 36rpx;
}

.switch-btn .switch-item {
    position: relative;
    text-align: center;
    line-height: 72rpx;
    flex: 1;
}

.switch-btn .active {
    color: #fff;
    background: #2660e7;
    border-radius: 40rpx;
}

.my-rank {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
    position: relative;
    height: 260rpx;
}

.my-rank .rank {
    width: 690rpx;
    height: 220rpx;
    border-radius: 20rpx;
    background: linear-gradient(45deg,#ffa973,#f53000);
    box-shadow: 0rpx 6rpx 6rpx 0rpx rgba(255,81,67,.2);
    margin-top: 10rpx;
    display: flex;
    align-items: center;
}

.my-rank .rank .rank-left {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 15rpx 0 30rpx;
}

.my-rank .rank .rank-left .Merriweather {
    width: 85rpx;
    height: 85rpx;
    border: 2rpx solid #ffe4da;
    border-radius: 50%;
    text-align: center;
    line-height: 85rpx;
}

.my-rank .rank .rank-right {
    display: flex;
    flex-direction: column;
    width: 535rpx;
}

.my-rank .rank .rank-right .rank-right-top {
    display: flex;
    align-items: center;
}

.my-rank .rank .rank-right .rank-right-top image {
    width: 80rpx;
    height: 80rpx;
    min-width: 80rpx;
    border: 4rpx solid #fff;
    border-radius: 50%;
    margin: 20rpx;
}

.my-rank .rank .rank-right .rank-right-top .user-name {
    width: 170rpx;
}

.my-rank .rank .rank-right .rank-right-top .score {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 350rpx;
}

.my-rank .rank .rank-right .rank-right-bottom {
    display: flex;
    align-items: center;
    border-top: 1rpx solid #ffa973;
    justify-content: space-between;
}

.my-rank .rank .rank-right .rank-right-bottom .icon-item {
    display: flex;
    align-items: center;
    margin-top: 15rpx;
}

.my-rank .rank .rank-right .rank-right-bottom .icon-item image {
    width: 32rpx;
    min-width: 32rpx;
    height: 32rpx;
    margin-right: 5rpx;
}

.my-rank .btm-tips {
    width: 690rpx;
    height: 220rpx;
    border-radius: 20rpx;
    background: #fff;
    box-sizing: border-box;
    opacity: 1;
    margin-top: 10rpx;
}

.nationwide-rank {
    display: flex;
    flex-direction: column;
    padding: 32rpx 30rpx;
    min-height: 500rpx;
    position: relative;
}

.nationwide-rank .list .rank:nth-child(1) .rank-left .Merriweather {
    border-color: #d9c38c;
    color: #d9c38c;
}

.nationwide-rank .list .rank:nth-child(2) .rank-left .Merriweather {
    border-color: #999;
    color: #999;
}

.nationwide-rank .list .rank:nth-child(3) .rank-left .Merriweather {
    border-color: #d5a187;
    color: #d5a187;
}

.nationwide-rank .list .rank:nth-child(-n+99) .rank-left .num {
    font-size: 60rpx;
}

.nationwide-rank .list .rank:nth-child(-n+999):nth-child(n+100) .rank-left .num {
    font-size: 40rpx;
}

.nationwide-rank .list .rank:nth-child(-n+9999):nth-child(n+1000) .rank-left .num {
    font-size: 30rpx;
}

.nationwide-rank .rank {
    width: 690rpx;
    height: 220rpx;
    border-radius: 20rpx;
    background: #fff;
    margin: 10rpx 0 20rpx;
