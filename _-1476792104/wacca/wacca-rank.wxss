}

.nationwide-rank .btm-tips {
    height: 550rpx;
    background: #fff;
    box-sizing: border-box;
    opacity: 1;
}

.nationwide-rank .btm-tips,.top3-user {
    width: 690rpx;
    border-radius: 20rpx;
    margin-top: 10rpx;
}

.top3-user {
    height: 537rpx;
    position: relative;
    box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(53,131,254,.22);
    margin-bottom: 30rpx;
    background: url("http://yyb-oss.universal-space.cn/imgs/record/Group 2.png?v=2");
    background-size: 100% 100%;
}

.top3-user .user {
    width: 230rpx;
    position: absolute;
}

.top3-user .user:nth-child(1) {
    left: 227rpx;
    top: 73rpx;
}

.top3-user .user:nth-child(1) .avatar {
    width: 140rpx;
    height: 140rpx;
}

.top3-user .user:nth-child(2) {
    left: 35rpx;
    top: 195rpx;
}

.top3-user .user:nth-child(2) .nickname {
    margin-top: 70rpx;
}

.top3-user .user:nth-child(3) {
    left: 429rpx;
    top: 257rpx;
}

.top3-user .user:nth-child(3) .nickname {
    line-height: 1;
    margin-top: 60rpx;
}

.top3-user .user .avatar {
    border-radius: 50%;
    overflow: hidden;
    margin: auto;
    width: 111rpx;
    height: 111rpx;
}

.top3-user .user .avatar image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.top3-user .user .nickname {
    margin-top: 80rpx;
    text-align: center;
    font-size: 26rpx;
    color: #3374fe;
}

.top3-user .user .score {
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-size: 32rpx;
    color: #3374fe;
}

.top3-user .user .score image {
    width: 26rpx;
    height: 26rpx;
}