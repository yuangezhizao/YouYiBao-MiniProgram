.hor_center {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
}

.var_center {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}

.both_center {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.back_warp {
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,0.5);
    z-index: 999;
    transition: all .3s ease;
}

.back_warp.hide {
    transform: scale(0);
}

.ellipsis-1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.ellipsis-2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
}

.statusBar {
    display: flex;
    justify-content: center;
    position: relative;
}

.statusBar .title {
    color: #fff;
    text-align: center;
    font-size: 36 rpx;
}

.Merriweather {
    font-family: 'Merriweather';
}

.btm-tips {
    padding: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    opacity: .6;
    font-size: 28rpx;
    color: #999;
}

.btm-tips image {
    width: 72rpx;
    height: 72rpx;
    margin-right: 10rpx;
}