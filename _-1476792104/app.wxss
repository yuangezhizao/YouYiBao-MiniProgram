.clearBtn,.form_container .form_button {
    background-color: transparent;
    padding: 0;
    margin: 0;
    border: 0;
    border-radius: 0;
    line-height: 1;
}

.disabled {
    background: #BBBBBB;
    color: #fff;
}

.form_container .form_button {
    width: 100%;
    max-width: 100vw;
    overflow-x: hidden;
}

.form_container .form_button:after {
    border: none;
    display: none;
}

.tabs {
    width: 100%;
    display: flex;
    height: 88rpx;
    line-height: 88rpx;
    margin-top: 38rpx;
    margin-bottom: 15rpx;
}

.tabs text {
    flex: 1;
    text-align: center;
    color: #4d4d4d;
    border-bottom: 4rpx solid transparent;
    font-size: 30rpx;
}

.tabs text.active {
    color: #3ca6fe;
    border-color: #3ca6fe;
}

.container {
    height: 100%;
    box-sizing: border-box;
}

page {
    background: #f2f2f2;
